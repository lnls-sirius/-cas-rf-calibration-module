
record(waveform, "RA-RaBO01:LLRFCalSys:Vals"){
    field(DTYP, "stream")
    field(SCAN, ".1 second")
    field(DTYP, "stream")
    field(INP,  "@RFCalibrationModule.proto getData $(PORT) $(A)")
    field(FTVL, "FLOAT")
    field(NELM, "17")
}


record(mbbi, "RA-RaBO01:LLRFCalSys:StatusCalOn"){
    field(INP, "RA-RaBO01:LLRFCalSys:Vals.VAL[0] CP MSS")

    field(ZRVL, "0")
    field(ONVL, "1")

    field(ZRST, "Off")
    field(ONST, "On")
}


record(calc, "RA-RaBO01:LLRFCalSys:PwrdBm1_ADC"){
    field(CALC, "((A/10)/4095.)*5.")
    field(INPA, "RA-RaBO01:LLRFCalSys:Vals.VAL[1] CP MSS")
}

record(calc, "RA-RaBO01:LLRFCalSys:PwrdBm1_CALC"){
    field(CALC, "A*(F**4) + B*(F**3) + C*(F**2) + D*F + E")

    field(INPA, "-4.6759")
    field(INPB, "26.107")
    field(INPC, "-53.648")
    field(INPD, "7.6581")
    field(INPE, "26.307")

    field(INPF, "RA-RaBO01:LLRFCalSys:PwrdBm1_ADC CP MSS")
}

record(calc, "RA-RaBO01:LLRFCalSys:PwrdBm1-Mon"){
    field(CALC, "(A>-42.0)?(A):(-Inf)")
    field(PREC, "2")
    field(EGU,  "dBm")
    field(INPA, "RA-RaBO01:LLRFCalSys:PwrdBm1_CALC CP MSS")
}


